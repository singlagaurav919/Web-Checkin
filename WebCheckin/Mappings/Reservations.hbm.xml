<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="WebCheckin" namespace="WebCheckin.Models">
	<class name="Reservations" table="Rez" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="RezId" type="int">
			<generator class="native" />
		</id>
		
		<property name="FirstName"  />
		<property name="LastName" />
		<property name="ArrivalDate" />
		<property name="DeptDate"  />
		<property name="NoAdult" />
		<property name="NoRoom" />
		<property name="RezStatus"  />
		<property name="Country" />
		<property name="ImageSrc" />
		<property name="Phone" />
		<property name="Email" />
	</class>
</hibernate-mapping>